<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<ui:with
		type="com.tinesoft.gwt.dialogs.showcase.client.resources.ShowcaseResources"
		field="res" />

	<g:FlowPanel>
		<g:HTMLPanel>
			<h1>MesssageBox Showcase</h1>
		</g:HTMLPanel>
		<g:FlowPanel styleName="{res.css.clear}">
			<g:FlowPanel styleName="{res.css.left}">
				<g:FlowPanel ui:field="infoMessageBox" styleName="{res.css.container}">
					<g:FlowPanel styleName="{res.css.left}">
						<g:Image resource="{res.infoIcon}"></g:Image>
					</g:FlowPanel>
					<g:FlowPanel styleName="{res.css.left}">
						<g:Label styleName="{res.css.title}">Information MessageBox</g:Label>
						<g:Label styleName="{res.css.content}">Click here to show an information dialog box.</g:Label>
					</g:FlowPanel>
					<g:FlowPanel styleName="{res.css.clear}"></g:FlowPanel>
				</g:FlowPanel>
			</g:FlowPanel>

			<g:FlowPanel styleName="{res.css.left}">
				<g:FlowPanel ui:field="askMessageBox" styleName="{res.css.container}">
					<g:FlowPanel styleName="{res.css.left}">
						<g:Image resource="{res.askIcon}"></g:Image>
					</g:FlowPanel>
					<g:FlowPanel styleName="{res.css.left}">
						<g:Label styleName="{res.css.title}">Question MessageBox</g:Label>
						<g:Label styleName="{res.css.content}">Click here to show a question dialog box.</g:Label>
					</g:FlowPanel>
					<g:FlowPanel styleName="{res.css.clear}"></g:FlowPanel>
				</g:FlowPanel>
			</g:FlowPanel>
		</g:FlowPanel>
		<g:FlowPanel styleName="{res.css.clear}">
			<g:FlowPanel styleName="{res.css.left}">
				<g:FlowPanel ui:field="warnMessageBox" styleName="{res.css.container}">
					<g:FlowPanel styleName="{res.css.left}">
						<g:Image resource="{res.warnIcon}"></g:Image>
					</g:FlowPanel>
					<g:FlowPanel styleName="{res.css.left}">
						<g:Label styleName="{res.css.title}">Warning MessageBox</g:Label>
						<g:Label styleName="{res.css.content}">Click here to show a warning dialog box.</g:Label>
					</g:FlowPanel>
					<g:FlowPanel styleName="{res.css.clear}"></g:FlowPanel>
				</g:FlowPanel>
			</g:FlowPanel>

			<g:FlowPanel styleName="{res.css.left}">
				<g:FlowPanel ui:field="errorMessageBox" styleName="{res.css.container}">
					<g:FlowPanel styleName="{res.css.left}">
						<g:Image resource="{res.errorIcon}"></g:Image>
					</g:FlowPanel>
					<g:FlowPanel styleName="{res.css.left}">
						<g:Label styleName="{res.css.title}">Error MessageBox</g:Label>
						<g:Label styleName="{res.css.content}">Click here to show an error dialog box.</g:Label>
					</g:FlowPanel>
					<g:FlowPanel styleName="{res.css.clear}"></g:FlowPanel>
				</g:FlowPanel>
			</g:FlowPanel>
		</g:FlowPanel>
		<g:FlowPanel styleName="{res.css.clear}">
			<g:FlowPanel styleName="{res.css.container}">
				<g:Hyperlink ui:field="backToMain">&lt;&lt; Back to main page</g:Hyperlink>
			</g:FlowPanel>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder>
