<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<ui:with
		type="com.tinesoft.gwt.dialogs.showcase.client.resources.ShowcaseResources"
		field="res" />

	<g:FlowPanel>
		<g:HTMLPanel>
			<h1>GWT Dialogs Showcase</h1>
		</g:HTMLPanel>

		<g:FlowPanel styleName="{res.css.clear}">
			<g:FlowPanel styleName="{res.css.left}">
				<g:FlowPanel ui:field="messageBoxShowcase" styleName="{res.css.container}">
					<g:FlowPanel styleName="{res.css.left}">
						<g:Image resource="{res.messageboxesIcon}"></g:Image>
					</g:FlowPanel>
					<g:FlowPanel styleName="{res.css.left}">
						<g:Label styleName="{res.css.title}">MessageBox Showcase</g:Label>
						<g:Label styleName="{res.css.content}">The standard error, warning,
						information,
						and question mark icons.</g:Label>
					</g:FlowPanel>
					<g:FlowPanel styleName="{res.css.clear}"></g:FlowPanel>
				</g:FlowPanel>
			</g:FlowPanel>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder>
